<% current = Time.now %>
<% match_status = Code.find_by_codeable_type_and_code('match_status', item.status).title %>
<% if item.doing?(current) %>
	<%= "状态: #{match_status} <em class='most'>[报名#{item.enroll_time_status(current)[2]} · 征集#{item.collect_time_status(current)[2]} · 投票#{item.vote_time_status(current)[2]}]</em>" %>
<% else %>
	<%= "状态: #{match_status}" %>
<% end %>