<% cache(:controller => "matches", :action => "profile", :user_id => session[:user_id], :part => "profile_6") do %>
<% if @player_contactors_set && @player_contactors_set.size > 0 %>
	<%= render :partial => 'layouts/items_sideblock', 
						 :locals => { :id => 'player_contactors', 
                                      :block_title => "参赛的伙伴们",
                                      :block_header_link => nil,
                                      :items_set => @player_contactors_set } %>
<% end %>
<% end %>

<% cache(:controller => "matches", :action => "profile", :part => "profile_7") do %>
<% if @highest_voted_entries && @highest_voted_entries.size > 0 %>
	<%= render :partial => 'layouts/ranklist_sideblock',
						 :locals => { :rank_list_title => @rank_list_title,
                                      :items_set => @highest_voted_entries } %>
<% end %>
<% end %>
