<% codeable_type = "contact_filter" %>
<% items_set_count = @contacts_set_count %>

<% if @user %>
	<% user = @user %>
	<% codeable_type += "_ta" %>
<% elsif params[:action] == 'mine' %>
	<% user = @current_user %>
	<% codeable_type += "_my" %>
<% end %>

<%= render :partial => "/layouts/items_filter_bar", 
					 :locals => { :user => user, 
					 							:codeable_type => codeable_type, 
					 							:items_set_count => items_set_count } %>
