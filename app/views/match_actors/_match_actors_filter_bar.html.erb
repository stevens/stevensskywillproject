<% items_set_count = @match_actors_set_count %>

<% if params[:filter] %>
  <% codeable_type = "match_#{params[:filter]}_filter" %>
  <% if @current_user %>
    <% codeable_type += "_my" %>
  <% end %>
<% end %>

<%= render :partial => "layouts/items_filter_bar", 
            :locals => { :user => nil,
                        :codeable_type => codeable_type,
                        :items_set_count => items_set_count } %>
