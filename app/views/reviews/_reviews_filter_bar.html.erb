<% codeable_type = "review_filter" %>
<% items_set_count = @reviews_set_count %>

<% if @current_user && @user == @current_user %>
  <% user = @current_user %>
  <% codeable_type += "_my" %>
<% elsif @user %>
  <% user = @user %>
  <% codeable_type += "_ta" %>
<% end %>

<%= render :partial => "layouts/items_filter_bar", 
            :locals => { :user => user,
                        :codeable_type => codeable_type,
                        :items_set_count => items_set_count } %>