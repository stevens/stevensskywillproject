<div class="content_block">
	
	<div class="block_header">
		<div class="block_header_link">
			<%= link_to SEE_CN, @self_url %> |
			<%= link_to strip_tags("更多#{PHOTO_CN} (#{@photos_set_count})"), @selfs_url %> |
			<%= link_to strip_tags("#{BACK_CN}#{@parent_name}"), @parent_url %>
		</div>
		<h1><%= strip_tags @block_title %></h1>
	</div>

	<div class="block_main">
		<%= render :partial => "/photos/photo_input", 
												    :locals => {:photoable => @parent_obj,
												 						    :photo => @photo, 
												 						    :is_new => false} %>
	</div>
	
</div>
