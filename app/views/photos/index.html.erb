<div class="content_block">
	
	<div class="block_header">
		<div class="block_header_link">
			<% if @parent_user && @parent_user == @current_user %>
				<%= link_to "#{ADD_CN}æ–°#{@self_name}", @new_self_url %> |	
			<% end %>
			<%= link_to "#{BACK_CN}#{@parent_name}", @parent_url %>
		</div>
		<h1><%= h @block_title %></h1>
	</div>
	
	<%= render :partial => "layouts/items_paginate",
						 :locals => {:items => @photos} %>

	<div class="block_main">
		<%= render :partial => "photos_matrix",
							 :locals => {:photos => @photos, 
							 						 :photos_count_per_row => MATRIX_ITEMS_COUNT_PER_ROW_M, 
							 						 :photoable => @parent_obj, 
							 						 :photo_link_url => false, 
							 						 :photo_style => 'matrix', 
							 						 :show_photo => true, 
							 						 :show_photo_todo => true} %>
	</div>
	
	<%= render :partial => "layouts/items_paginate",
						 :locals => {:items => @photos} %>

</div>